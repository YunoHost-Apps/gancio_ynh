{
    "baseurl": "https://__DOMAIN__",
    "server": {
        "host": "127.0.0.1",
        "port": "__PORT__"
        },
    "log_level": "debug",
    "log_path": "/var/log/__APP__/__APP__.log",
    "db": {
        "dialect": "postgres",
        "storage": "",
        "host": "localhost",
        "database": "__DB_NAME__",
        "username": "__DB_USER__",
        "password": "__DB_PWD__",
        "logging": false,
        "dialectOptions": {
            "autoJsonMap": true
        }
    },
    "user_locale": "__DATA_DIR__/user_locale",
    "upload_path": "__DATA_DIR__/uploads",
    "plugins_path": "__DATA_DIR__/plugins"
}
